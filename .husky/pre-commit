if git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g' | grep -en '\.(js|jsx|ts|tsx)?$'; then
  pnpm exec prettier --write $(git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g' | grep -e '\.(js|jsx|ts|tsx)?$' )
fi
pnpm exec eslint $(git diff --cached --name-only --diff-filter=ACMR | sed 's| |\\ |g' | grep -e '\.(js|jsx|ts|tsx)?$')
